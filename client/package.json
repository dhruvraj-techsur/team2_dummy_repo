    "vite": "6.3.5",
    "typescript": "~5.8.3",
    "@types/react": "18.0.38",
    "@types/react-dom": "19.1.6",
    "@types/node": "^20.0.0",
    "husky": "9.1.7",
    "csstype": "3.0.0",
    "@babel/parser": "7.28.0",
    "@babel/types": "7.28.0",
    "undici-types": "7.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "@types/scheduler": "< 0.23.0"
