{
  "name": "templates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "7.5.1",
    "vite": "6.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "4.6.0",
    "typescript": "5.8.3",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.1.6",
    "@types/node": "24.0.13",
    "husky": "9.1.7"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,css,scss}": [
      "prettier --write"
    ]
