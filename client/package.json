{
  "name": "templates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^4.3.9"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "files": [
    "src",
    "public",
    "index.html"
  ],
  "keywords": [
    "react",
    "vite",
    "typescript",
    "eslint",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/yourrepo.git"
  },
  "author": "Your Name <youremail@example.com>",
  "homepage": "https://yourhomepage.com",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}